<!doctype html>
<html lang="en">
    <head>
        <!-- Required meta tags -->
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
     
        <title>Anfra KYC</title>
    
        <!-- Bootstrap CSS -->
        <link rel="stylesheet" href="/static/css/bootstrap.min.css">
    
        <!-- External Css -->
        <link rel="stylesheet" href="/static/css/line-awesome.min.css">
    
        <!-- Custom Css -->
        <link rel="stylesheet" type="text/css" href="/static/css/main.css">
        <link rel="stylesheet" type="text/css" href="/static/css/kyc-2.css">
    
        <!-- Fonts -->
        <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@400;500;700&amp;display=swap" rel="stylesheet">
    
        <!-- Favicon -->
        <link rel="icon" href="/static/images/favicon.png">
        <link rel="apple-touch-icon" href="/static/images/apple-touch-icon.png">
        <link rel="apple-touch-icon" sizes="72x72" href="/static/images/icon-72x72.png">
        <link rel="apple-touch-icon" sizes="114x114" href="/static/images/icon-114x114.png">    
    </head>
    <body>
        <div class="ugf-nav">
            <div class="container">
              <div class="row">
                <div class="col-lg-8 offset-lg-2">
                  <div class="navigation">
                    <div class="logo">
                      <a href="https://uigaint.com/demo/html/anfraa/index.html"><img src="/static/images/logo-dark.png" class="img-fluid" alt=""><span>KYC</span></a>
                    </div>
                    <div class="nav-btns">
                      <a href="https://uigaint.com/demo/html/anfraa/index.html" class="back"><span class="text">Back to Main</span> Demo</a>
                      <a href="https://themeforest.net/item/anfra-questionnaire-form-wizard/29917051" class="get">Get Anfra</a>
                    </div>
                  </div>
                </div>
              </div>
            </div>
        </div>
        <div class="ugf-bg ufg-main-container">
            <div class="container">
                <div class="row">
                    <div class="col-lg-8 offset-lg-2 p-sm-0">
                        <div class="ugf-form">
                            <form action="#" id="commentForm">
                                <div class="input-block">
                                    <h4>Business Information</h4>
                                    <div class="row">
                                        <div class="col-md-6 p-sm-0">
                                            <div class="form-group">
                                              <label for="inputFname">Company Name</label>
                                              <input type="text" name="cName" class="form-control"  placeholder="Company Brand Name" required>
                                            </div>
                                        </div>
                                        <div class="col-md-6 p-sm-0">
                                            <div class="form-group">
                                              <label for="inputFname">Administrative Email</label>
                                              <input type="text" name="adminEmail" class="form-control" placeholder="administrator@email.com" required>
                                            </div>
                                        </div>
                                        <div class="col-md-12 p-sm-0">
                                          <div class="form-group">
                                            <label for="inputFname">Address</label>
                                            <input type="text" name="address" class="form-control" placeholder="Floor 2, Talyor Swift Building " required>
                                          </div>
                                      </div>
                                        <div class="col-md-6 p-sm-0">
                                            <div class="form-group">
                                              <label for="inputFname">City</label>
                                              <input type="text" name="city" class="form-control" placeholder="Ikeja" required>
                                            </div>
                                        </div>
                                        <div class="col-md-6 p-sm-0">
                                            <div class="form-group">
                                              <label for="inputFname">State</label>
                                              <input type="text" name="state" class="form-control" placeholder="Lagos" required>
                                            </div>
                                        </div>
                                        
                                        <div class="col-md-6 p-sm-0">
                                            <div class="form-group">
                                              <label for="inputFname">Password</label>
                                              <input type="password" name="password" class="form-control" placeholder="Desired Password" required>
                                            </div>
                                        </div>
                                        <div class="col-md-6 p-sm-0">
                                            <div class="form-group">
                                              <label for="inputFname">Confirm Password</label>
                                              <input type="password" name="cpassword" class="form-control" placeholder="Confirm Password" required>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="input-block">
                                    <h4>Website Information</h4>
                                    <div class="form-group">
                                        <label for="">Domain Information</label>
                                        <div class="check-gender">
                                          <div class="custom-radio">
                                            <input type="radio" name="gender" class="custom-control-input" id="Gmale" required>
                                            <label class="custom-control-label" for="Gmale">New</label>
                                          </div>
                                          <div class="custom-radio">
                                            <input type="radio" name="gender" class="custom-control-input" id="Gfemale" required>
                                            <label class="custom-control-label" for="Gfemale">Existing</label>
                                          </div>
                                          <input type="text" name="domain" class="form-control" placeholder="Domain Name" required>
                                        </div>
                                      </div>
                                    <div class="row">
                                      <div class="col-md-6 p-sm-0">
                                        <div class="form-group">
                                          <label for="inputFname">Website Tagline</label>
                                          <input type="text" name="tagline" class="form-control" placeholder="Website Tagline" required>
                                        </div>
                                      </div>
                                      <div class="col-md-6 p-sm-0">
                                        <div class="form-group country-select">
                                          <label for="inputCountry">Would you consider digital Marketing Services?</label>
                                          <div class="select-input choose-country">
                                            <span></span>
                                            <select id="inputCountry" name="Nationality" class="form-control" required >
                                              <option>Select an option</option>
                                              <option value="maybe">Not now, I may consider this later.</option>
                                              <option value="no">No, I am not interested.</option>
                                              <option value="yes">Yes, I am interested in this service</option>"
                                            </select>
                                          </div>
                                        </div>
                                      </div>
                                    </div>
                                </div>
                                <div class="input-block">
                                  <div class="documents-upload-wrap">
                                    <div class="tab-content" id="pills-tabContent">
                                      <div class="tab-pane fade show active" id="passport" role="tabpanel" aria-labelledby="home-tab">
                                        <div class="documents-upload">
                                          <div class="upload-item">
                                            <input type="file" id="input1" class="input-file" accept="image/*" required hidden />
                                            <label class="btn-upload" for="input1" role="button">
                                              <img src="/static/images/cloud.png">
                                              <div id="p-input1" class="preview-box"></div>
                                            </label>
                                            <div class="upload-direction"><span class="text">Upload Content</span><a href="#"><img src="/static/images/download.png" alt=""></a></div>
                                          </div>
                                          <div class="upload-item">
                                            <input type="file" id="input2" class="input-file" accept="image/*" required hidden />
                                            <label class="btn-upload" for="input2" role="button">
                                              <img src="/static/images/cloud.png">
                                              <div  id="p-input2" class="preview-box"></div>
                                            </label>
                                            <div class="upload-direction"><span class="text">Upload Logo</span><a href="#"><img src="/static/images/download.png" alt=""></a></div>
                                          </div>
                                          <div class="upload-item"></div>
                                        </div>
                                        <div class="conditions">
                                          
                                          <ul>
                                            <li>List of services</li>
                                            <!-- <li class="complete">File accepted: JPEG/JPG/PNG (Max size: 250 KB)</li>
                                            <li>Document should be good condition</li>
                                            <li>Document must be  valid period</li>
                                            <li>Face must be clear visible</li>
                                            <li>Note has today’s date</li> -->
                                          </ul>
                                        </div>
                                      </div>
                                    </div>
                                  </div>  
                                  <div class="form-group">
                                    <div class="custom-checkbox">
                                      <input type="checkbox" name="checkBox" class="custom-control-input" id="customControlValidation1" required>
                                      <label class="custom-control-label" for="customControlValidation1">I accept the <a href="#">Terms &amp; Conditions</a> and <a href="#">Privacy policy</a></label>
                                    </div>
                                  </div>
                                </div>
                                <button class="btn">Submit Information</button>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- jQuery first, then Popper.js, then Bootstrap JS -->
        <script src="/static/js/jquery.min.js"></script>
        <script src="/static/js/popper.min.js"></script>
        <script src="/static/js/bootstrap.min.js"></script>
        <script src="/static/js/jquery.validate.min.js"></script>

        <script src="/static/js/custom.js"></script>
    </body>